@page "/"
    
    <img src="/css/LogoExample3.png" width=100 height=100/>

    <div class=buttons-container>
        
        <MudButton Link="CreateRoom" Variant="Variant.Filled"  Class="buttons" Style="width: 300px; height: 300px; min-width: 300px;
">
        Create Room</MudButton>
        
        <MudButton Link="JoinRoom" Variant="Variant.Filled" Class="buttons" Style="font-size: 24px;">
        Join Room</MudButton>
    </div>

    <div>
        <input value = Test1 onchange="Update"/>
    </div>

    @code{
    private string Test1 = "";
    void Update(ChangeEventArgs e)
    {
        if(e is not null && e.Value is not null)
        {
            Test1 = e.Value.ToString();
            StateHasChanged();
        }  
    }
    }



